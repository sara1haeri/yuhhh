/* This program will have Karel run around the racetrack
 * 8 times. */
function start() {
    for(var i = 0; i <8; i++){
        if(noBallsPresent()){
            put8();
        }
        while(frontIsClear()){
            forward();
        
        }
        turnLeft();
    }
}
function put8(){
    putBall();
    putBall();
    putBall();
    putBall();
    putBall();
    putBall();
    putBall();
    putBall();
}
function forward(){
    move();
}
