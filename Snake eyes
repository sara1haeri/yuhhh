function start(){
    var rolls = 0;
	while(true){
    	var roll1 = Randomizer.nextInt(1, 6);
    	var roll2 = Randomizer.nextInt(1, 6);
    	rolls ++;
    	var snake = roll1 == 1 && roll2 == 1;
    	println("Rolled: " + roll1 + ", " + roll2);
    	if(snake){
    	    println("It took you " + rolls + " rolls to get snake eyes. ");
    	    break;
    	}
    }
}
