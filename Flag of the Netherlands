/* This program should draw the Netherlands flag. The
 * top third of the canvas is red, the middle third
 * is white, and the bottom third is blue. */
function start(){
	var rect1 = new Rectangle(getWidth(),getHeight()/3);
	rect1.setColor(Color.red);
	add(rect1);
	
	var rect2 = new Rectangle(getWidth(),getHeight()/3);
	rect2.setPosition(0,320);
	rect2.setColor(Color.blue);
	add(rect2);
}
